{
  "name": "campus-rent",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --max-warnings=0 \"{src,apps,libs,test}/**/*.ts\" --debug",
    "lint:fix": "eslint --max-warnings=0 \"{src,apps,libs,test}/**/*.ts\" --debug --fix",
    "format": "biome format check .",
    "format:check": "bunx biome check . --verbose",
    "format:fix": "bunx biome format --write --no-errors-on-unmatched",
    "check": "bun run format:check && bun run lint",
    "check:fix": "bun run format:fix && bun run lint:fix"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "biome check --verbose",
      "eslint --max-warnings=0"
    ],
    "*.html": [
      "eslint --max-warnings=0"
    ],
    "*.{json,scss,css}": [
      "biome format --write --no-errors-on-unmatched"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.14",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^20.3.14",
    "@angular/compiler": "^20.3.14",
    "@angular/core": "^20.3.14",
    "@angular/forms": "^20.3.14",
    "@angular/material": "^19.2.19",
    "@angular/material-moment-adapter": "19.2.19",
    "@angular/platform-browser": "^20.3.14",
    "@angular/platform-browser-dynamic": "^20.3.14",
    "@angular/platform-server": "^20.3.14",
    "@angular/router": "^20.3.14",
    "@angular/service-worker": "^20.3.14",
    "@angular/ssr": "^20.3.12",
    "express": "^4.18.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "rxjs": "^7.8.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.19.4",
    "@analogjs/vite-plugin-angular": "^1.19.4",
    "@angular-eslint/builder": "18.3.0",
    "@angular-eslint/eslint-plugin": "^20.1.1",
    "@angular-eslint/eslint-plugin-template": "^20.1.1",
    "@angular-eslint/schematics": "18.3.0",
    "@angular-eslint/template-parser": "^20.1.1",
    "@angular/build": "^20.3.12",
    "@angular/cli": "^20.3.12",
    "@angular/compiler-cli": "^20.3.14",
    "@angular/language-service": "^20.3.14",
    "@biomejs/biome": "^2.1.4",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/angular": "^17.0.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "latest",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.17.20",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10",
    "eslint": "8",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.32.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^3",
    "typescript": "5.9.3",
    "vitest": "^3.2.4"
  }
}
