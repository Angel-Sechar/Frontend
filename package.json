{
  "name": "campus-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "biome format --write .",
    "format:check": "bunx biome check",
    "format:fix": "bunx biome format --write --no-errors-on-unmatched",
    "check": "bun run format:check && bun run lint",
    "check:fix": "bun run format:fix && bun run lint:fix",
    "serve:ssr": "node dist/frontend/server/server.mjs"
  },
  "lint-staged": {
    "*.{ts,js,html}": [
      "bunx biome check",
      "eslint --max-warnings=0 --max-errors=0"
    ],
    "*.{json,scss,css}": [
      "biome format --write --no-errors-on-unmatched"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^18.2.13",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/material": "^18.2.13",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/platform-server": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@angular/service-worker": "^18.2.13",
    "@angular/ssr": "^18.2.0",
    "express": "^4.18.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.19.4",
    "@analogjs/vite-plugin-angular": "^1.19.4",
    "@angular-devkit/build-angular": "^18.2.13",
    "@angular-eslint/builder": "18.3.0",
    "@angular-eslint/eslint-plugin": "^20.1.1",
    "@angular-eslint/eslint-plugin-template": "^20.1.1",
    "@angular-eslint/schematics": "18.3.0",
    "@angular-eslint/template-parser": "^20.1.1",
    "@angular/cli": "^18.2.13",
    "@angular/compiler-cli": "^18.2.13",
    "@biomejs/biome": "^2.1.4",
    "@eslint/eslintrc": "^3.3.1",
    "@testing-library/angular": "^18.0.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.17.20",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "8",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.32.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "typescript": "~5.4.2",
    "vitest": "^3.2.4"
  },
  "resolutions": {
    "eslint": "^8.57.0",
    "@angular/core": "18.2.13",
    "@angular/common": "18.2.13",
    "@angular/router": "18.2.13",
    "@angular/platform-browser": "18.2.13"
  }
}
